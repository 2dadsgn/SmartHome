<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#43BEE8</item>
        <item name="colorPrimaryDark">#30B8E6</item>
        <item name="colorAccent">#43BEE8</item>
        <item name="android:navigationBarColor" >#43BEE8</item>
        <item name="colorControlHighlight">#43BEE8</item>
        <item name="android:statusBarColor">#43BEE8</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
